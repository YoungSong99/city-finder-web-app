<h1 class="sub-title">Compare cities you picked</h1>
<div class="row">
  <%= search_form_for @q, url: comparison_path, method: :get do |form| %>
    <div class="grid">
      <%= form.text_field :city_name_cont, placeholder: "Search by City Name", class: 'search search-field'%>
      <%= form.submit 'Search', class: 'btn primary search-btn' %>
    </div>
  <% end %>
</div>
